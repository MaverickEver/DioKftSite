@model DioKftSite.Models.Contact

<div class="col-xs-12 col-sm-3">
    <div class="row">
        <img style="margin-left:auto;margin-right:auto;display:block;" src="@Url.Content(Model.ImageUrl)" class="img-responsive img-circle" width="150" height="150">
    </div>
    <div class="row">
        <div style="text-align:center">@Model.Name</div>
    </div>
    <div class="row">
        <div style="text-align:center">@Model.Role</div>
    </div>
    <div class="row">
        <div style="text-align:center">@Model.PhoneNumber</div>
    </div>
    <div class="row">
        <div style="text-align:center">@Model.Email</div>
    </div>
</div>

